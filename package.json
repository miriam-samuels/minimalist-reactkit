{
  "name": "minimalist-reactkit",
  "version": "1.1.9",
  "description": "Lightweight, flexible and highly customizable reusable components for React-devs designed for easy integration into your projects, providing a clean and modern aesthetic.",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "imports": {
    "#*": [
      "./main/*",
      "./main/*.ts",
      "./main/*.tsx"
    ]
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "build-lib": "rollup -c",
    "test": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miriam-samuels/minimalist-reactkit.git"
  },
  "keywords": [
    "typescript",
    "react",
    "reusable",
    "components",
    "lightweight",
    "flexible",
    "easily",
    "customizable",
    "restylable"
  ],
  "author": "Miriam",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/miriam-samuels/minimalist-reactkit/issues"
  },
  "homepage": "https://github.com/miriam-samuels/minimalist-reactkit#readme",
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.3",
    "babel-loader": "^9.1.3",
    "react": "^18.2.0",
    "rollup": "^2.79.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.66.1",
    "typescript": "^5.2.2"
  }
}